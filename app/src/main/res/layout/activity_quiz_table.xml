                <Button

                    android:id="@+id/button1"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Big O "
                    android:onClick="bigo"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button2"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Interview Tips"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button3"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Array"
                    android:onClick="array"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button4"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Linked List"
                    android:onClick="linkedlist"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button5"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="HashMap/Hashtable"
                    android:onClick="hashmap"
                    android:textSize="10sp" />
                <Button

                    android:id="@+id/button6"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Stack/Queue"
                    android:onClick="stackq"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button7"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Strings"
                    android:onClick="string"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button8"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Binary Tree"
                    android:onClick="binary"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button9"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Recursion"
                    android:onClick="recursion"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button10"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Inheritance"
                    android:onClick="inheritance"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button11"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Sorting"
                    android:onClick="sorting"
                    android:textSize="18sp" />
                <Button

                    android:id="@+id/button12"
                    android:layout_height="100sp"
                    android:layout_columnWeight="1"
                    android:layout_margin="5sp"
                    android:background="#ffffff"
                    android:text="Revision"
                    android:onClick="revision"
                    android:textSize="18sp" />



